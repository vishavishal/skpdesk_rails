<div id="project_list">
  <h1> Project list</h1>
  <table class="table table-dark table-bordered">
    <thead>
      <tr>
        <th scope="col">Title</th>
        <th scope="col">Email</th>
        <th scope="col">Type</th>
      </tr>
    </thead>
    <tbody>
      <% @projects.each do |project| %>
        <tr>
          <td> <%= project.title %> </td>
          <td> <%= project.client %> </td>
          <td> <%= project.project_type %> </td>
        </tr>
      <% end %>
    </tbody>
  </table>  
</div>